div
  h2.alert.alert-danger(ng-if='errorMessage') {{errorMessage}}
  h2.alert.alert-success(ng-if='successMessage') {{successMessage}}
  h2 Templates
  button(class="btn btn-default" ui-sref="templateCreate()") Create Template
  table.table
    tr
      td ID
      td Name
      td 
    tr(ng-repeat='template in templates')
      td
        a(ui-sref="templateViewUpdate({templateId: template.id})") {{template.id}}
      td {{template.name}}
      td 
        button(class="btn btn-default"  ng-click="delete(template.id)") Delete
