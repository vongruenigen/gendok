div
  h2.alert.alert-danger(ng-if='error_message') {{error_message}}
  h2.alert.alert-success(ng-if='success_message') {{success_message}}
  h2 Templates
  table
    tr
      td ID
      td Name
    tr(ng-repeat='template in templates')
      td
        a(ui-sref="template({templateId: template.id})") {{template.id}}
      td {{template.name}}
      td 
        a(href="" ng-click="delete(template.id)") x
